  file = {/home/phc/Dropbox/ResearchPapers/Journal of Theoretical Biology/2010/Chan_et_al_Force-extension_formula_for_the_worm-like_chain_model_from_a_variational_2010_Journal_of_Theoretical_Biology.pdf}
