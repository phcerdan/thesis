  year = {1996},
  keywords = {Biomedical imaging,Euclidean distance,Fires,Helium,Joining processes,Mathematics,Radiology,Shape,Skeleton,approximate distance metrics,connected skeletons,connected thinning,discrete Euclidean distance maps,exact Euclidean metric,image,image recognition,maximal disks,minimisation,shape analysis,skeletons generation,steepest ascent},
