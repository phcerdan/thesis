  file = {/home/phc/Dropbox/ResearchPapers/Pattern Recognition Letters/Couprie_Asymmetric_parallel_3D_thinning_scheme_and_algorithms_based_on_isthmuses_Pattern_Recognition_Letters.pdf}
}

