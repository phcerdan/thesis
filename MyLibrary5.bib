@article{hauser_role_2012,
  title = {The Role of Feedback in Morphological Computation with Compliant Bodies},
  volume = {106},
  issn = {0340-1200, 1432-0770},
  doi = {10.1007/s00422-012-0516-4},
  abstract = {The generation of robust periodic movements of complex nonlinear robotic systems is inherently difficult, especially, if parts of the robots are compliant. It has previously been proposed that complex nonlinear features of a robot, similarly as in biological organisms, might possibly facilitate its control. This bold hypothesis, commonly referred to as morphological computation, has recently received some theoretical support by Hauser et al. (Biol Cybern 105:355\textendash{}370, doi:10.1007/s00422-012-0471-0, 2012). We show in this article that this theoretical support can be extended to cover not only the case of fading memory responses to external signals, but also the essential case of autonomous generation of adaptive periodic patterns, as, e.g., needed for locomotion. The theory predicts that feedback into the morphological computing system is necessary and sufficient for such tasks, for which a fading memory is insufficient. We demonstrate the viability of this theoretical analysis through computer simulations of complex nonlinear mass\textendash{}spring systems that are trained to generate a large diversity of periodic movements by adapting the weights of a simple linear feedback device. Hence, the results of this article substantially enlarge the theoretically tractable application domain of morphological computation in robotics, and also provide new paradigms for understanding control principles of biological organisms.},
  language = {en},
  number = {10},
  journal = {Biological Cybernetics},
  author = {Hauser, Helmut and Ijspeert, Auke J. and F{\"u}chslin, Rudolf M. and Pfeifer, Rolf and Maass, Wolfgang},
  month = nov,
  year = {2012},
  keywords = {Computer Appl. in Life Sciences,Limit cycles,Morphological computation,Neurosciences,Nonlinear system,Statistical Physics; Dynamical Systems and Complexity,bioinformatics,compliant robots,neurobiology},
  pages = {595--613},
  file = {/home/phc/Dropbox/ResearchPapers/Biological Cybernetics/2012/Hauser_et_al_The_role_of_feedback_in_morphological_computation_with_compliant_bodies_2012_Biol_Cybern.pdf}
