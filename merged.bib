  file		= {Wang_et_al_Stretching_DNA_with_optical_tweezers_1997_Biophys_J.pdf:/home/phc/Dropbox/ResearchPapers/Biophysical
		  Journal/1997/Wang_et_al_Stretching_DNA_with_optical_tweezers_1997_Biophys_J.pdf:application/pdf}
