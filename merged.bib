@Article{	  barbero_modular_2014,
  title		= {Modular Proximal Optimization for Multidimensional
		  Total-Variation Regularization},
  abstract	= {One of the most frequently used notions of "structured
		  sparsity" is that of sparse (discrete) gradients, a
		  structure typically elicited through
		  $\backslash$emph\{Total-Variation (TV)\} regularizers. This
		  paper focuses on anisotropic TV-regularizers, in particular
		  on \$$\backslash$ell\_p\$-norm $\backslash$emph\{weighted
		  TV regularizers\} for which it develops efficient
		  algorithms to compute the corresponding proximity
		  operators. Our algorithms enable one to scalably
		  incorporate TV regularization of vector, matrix, or tensor
		  data into a proximal convex optimization solvers. For the
		  special case of vectors, we derive and implement a highly
		  efficient weighted 1D-TV solver. This solver provides a
		  backbone for subsequently handling the more complex task of
		  higher-dimensional (two or more) TV by means of a modular
		  proximal optimization approach. We present numerical
		  experiments that demonstrate how our 1D-TV solver matches
		  or exceeds the best known 1D-TV solvers. Thereafter, we
		  illustrate the benefits of our modular design through
		  extensive experiments on: (i) image denoising; (ii) image
		  deconvolution; and (iii) four variants of fused-lasso. Our
		  results show the flexibility and speed our TV solvers offer
		  over competing approaches. To underscore our claims, we
		  provide our TV solvers in an easy to use multi-threaded C++
		  library (which also aids reproducibility of our results).},
  timestamp	= {2017-10-02T10:09:33Z},
  archiveprefix	= {arXiv},
  eprinttype	= {arxiv},
  eprint	= {1411.0589},
  primaryclass	= {math, stat},
  urldate	= {2017-10-02},
  journal	= {arXiv:1411.0589 [math, stat]},
  author	= {Barbero, {\'A}lvaro and Sra, Suvrit},
  month		= nov,
  year		= {2014},
  keywords	= {Mathematics - Optimization and Control,Statistics -
		  Machine Learning},
  file		= {Barbero_Modular_proximal_optimization_for_multidimensional_total-variation_2014_.pdf:/home/phc/.mozilla/firefox/18x8zg4b.default/zotero/storage/QSF9DHJ8/Barbero_Modular_proximal_optimization_for_multidimensional_total-variation_2014_.pdf:application/pdf}
}

