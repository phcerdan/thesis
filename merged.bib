  number	= {5},
  urldate	= {2017-10-19},
  journal	= {Biophysical Journal},
  author	= {Magatti, Davide and Molteni, Matteo and Cardinali, Barbara
		  and Rocco, Mattia and Ferri, Fabio},
  month		= mar,
  year		= {2013},
  keywords	= {in-silico,network,polymer},
  pages		= {1151--1159}
}

