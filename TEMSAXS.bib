  month = may,
  year = {2014},
  note = {00006},
  keywords = {atomic force microscopy,carbohydrates,coil–helix transition,conformation analysis,polymer physics},
  pages = {5376--5379},
  file = {Full Text PDF:/home/phc/.mozilla/firefox/18x8zg4b.default/zotero/storage/4R9B3VMJ/Schefer et al. - 2014 - Unravelling Secondary Structure Changes on Individ.pdf:application/pdf;Snapshot:/home/phc/.mozilla/firefox/18x8zg4b.default/zotero/storage/XDMFAG6K/abstract.html:text/html}
