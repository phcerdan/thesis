@article{magatti_modeling_2013,
  title = {Modeling of {{Fibrin Gels Based}} on {{Confocal Microscopy}} and {{Light}}-{{Scattering Data}}},
  volume = {104},
  issn = {0006-3495},
  doi = {10.1016/j.bpj.2013.01.024},
  abstract = {Fibrin gels are biological networks that play a fundamental role in blood coagulation and other patho/physiological processes, such as thrombosis and cancer. Electron and confocal microscopies show a collection of fibers that are relatively monodisperse in diameter, not uniformly distributed, and connected at nodal points with a branching order of $\sim$3\textendash{}4. Although in the confocal images the hydrated fibers appear to be quite straight (mass fractal dimension Dm~= 1), for the overall system 1$<$Dm$<$2. Based on the confocal images, we developed a method to generate three-dimensional (3D) in~silico gels made of cylindrical sticks of diameter d, density $\rho$, and average length 〈L〉, joined at randomly distributed nodal points. The resulting 3D network strikingly resembles real fibrin gels and can be sketched as an assembly of densely packed fractal blobs, i.e., regions of size $\xi$, where the fiber concentration is higher than average. The blobs are placed at a distance $\xi$0 between their centers of mass so that they are overlapped by a factor $\eta$~= $\xi$/$\xi$0 and have Dm $\sim$1.2\textendash{}1.6. The in~silico gels' structure is quantitatively analyzed by its 3D spatial correlation function g3D(r) and corresponding power spectrum I(q)~= FFT3D[g3D(r)], from which $\rho$,~d, Dm, $\eta$, and $\xi$0 can be extracted. In particular, $\xi$0 provides an excellent estimate of the gel mesh size. The in~silico gels' I(q) compares quite well with real gels' elastic light-scattering measurements. We then derived an analytical form factor for accurately fitting the scattering data, which allowed us to directly recover the gels' structural parameters.},
  timestamp = {2017-10-19T07:35:45Z},
  number = {5},
  urldate = {2017-10-19},
  journal = {Biophysical Journal},
  author = {Magatti, Davide and Molteni, Matteo and Cardinali, Barbara and Rocco, Mattia and Ferri, Fabio},
  month = mar,
  year = {2013},
  keywords = {in-silico,network,polymer},
  pages = {1151--1159}
}

@article{broedersz_filament-length-controlled_2012,
  title = {Filament-{{Length}}-{{Controlled Elasticity}} in {{3D Fiber Networks}}},
  volume = {108},
  doi = {10.1103/PhysRevLett.108.078102},
  abstract = {We present a model for disordered 3D fiber networks to study their linear and nonlinear elasticity. In contrast to previous 2D models, these 3D networks with binary crosslinks are underconstrained with respect to fiber stretching elasticity, suggesting that bending may dominate their response. We find that such networks exhibit a bending-dominated elastic regime controlled by fiber length, as well as a crossover to a stretch-dominated regime for long fibers. Finally, by extending the model to the nonlinear regime, we show that these networks become intrinsically nonlinear with a vanishing linear response regime in the limit of flexible or long filaments.},
  timestamp = {2017-10-19T08:07:07Z},
  number = {7},
  urldate = {2017-10-19},
